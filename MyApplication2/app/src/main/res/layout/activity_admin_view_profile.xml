<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin.Admin_ViewProfile">

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="89dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="65dp"
        android:text="Role:"
        app:layout_constraintBottom_toTopOf="@+id/admin_update"
        app:layout_constraintEnd_toStartOf="@+id/admin_role"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView25" />

    <Button
        android:id="@+id/admin_changepass"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:text="Change Password"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="454dp" />

    <TextView
        android:id="@+id/admin_password"
        android:layout_width="133dp"
        android:layout_height="27dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="128dp"
        android:inputType="textPassword"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/admin_phoneno" />

    <TextView
        android:id="@+id/admin_name"
        android:layout_width="142dp"
        android:layout_height="27dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="119dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/admin_phoneno"
        android:layout_width="162dp"
        android:layout_height="28dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="99dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/admin_email" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="58dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="30dp"
        android:text="Phone no:"
        app:layout_constraintEnd_toStartOf="@+id/admin_phoneno"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView26" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="79dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="30dp"
        android:text="Name:"
        app:layout_constraintEnd_toStartOf="@+id/admin_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/admin_email"
        android:layout_width="142dp"
        android:layout_height="27dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="119dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/admin_name" />

    <TextView
        android:id="@+id/admin_role"
        android:layout_width="142dp"
        android:layout_height="27dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="119dp"
        android:text="Textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/admin_password" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="54dp"
        android:layout_marginTop="49dp"
        android:layout_marginEnd="30dp"
        android:text="Password:"
        app:layout_constraintEnd_toStartOf="@+id/admin_password"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView23" />

    <Button
        android:id="@+id/admin_update"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:text="Update Particulars"
        app:layout_constraintEnd_toEndOf="@+id/admin_changepass"
        app:layout_constraintStart_toStartOf="@+id/admin_changepass"
        tools:layout_editor_absoluteY="409dp" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="82dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="30dp"
        android:text="Email:"
        app:layout_constraintEnd_toStartOf="@+id/admin_email"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView24" />

    <Button
        android:id="@+id/btn_homepage3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="170dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="153dp"
        android:layout_marginBottom="152dp"
        android:text="Home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/admin_changepass" />
</androidx.constraintlayout.widget.ConstraintLayout>