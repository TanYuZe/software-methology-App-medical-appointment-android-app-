<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pharmacist.Pharmacist_Main">

    <Button
        android:id="@+id/Pharmacist_Logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="157dp"
        android:layout_marginEnd="161dp"
        android:layout_marginBottom="58dp"
        android:text="Logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/PharmacistProfile"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="172dp"
        android:layout_marginEnd="107dp"
        android:layout_marginBottom="42dp"
        android:text="Profile"
        app:layout_constraintBottom_toTopOf="@+id/ViewPresc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/RetrievePresc"
        android:layout_width="207dp"
        android:layout_height="56dp"
        android:layout_marginStart="106dp"
        android:layout_marginTop="63dp"
        android:layout_marginEnd="106dp"
        android:text="Retrieve User Prescription"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PharmacistProfile" />

    <Button
        android:id="@+id/prescdata_btn"
        android:layout_width="210dp"
        android:layout_height="63dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="65dp"
        android:layout_marginEnd="97dp"
        android:layout_marginBottom="156dp"
        android:text="Prescription database"
        app:layout_constraintBottom_toTopOf="@+id/Pharmacist_Logout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/RetrievePresc" />
</androidx.constraintlayout.widget.ConstraintLayout>